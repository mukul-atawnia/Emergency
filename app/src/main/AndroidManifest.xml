            android:label="@string/title_activity_sqlite__database" >
        </activity>

        <activity android:name=".NewContactActivity"></activity>

        <activity
            android:name=".Contact_Search"
            android:label="@string/title_activity_contact__search" >
        </activity>
        <activity
            android:name=".Contact_Update"
            android:label="@string/title_activity_contact__update" >
        </activity>
        <activity android:name=".HospitalAddress" />
        <activity android:name=".HospitalsList" />

        <meta-data android:name="com.google.android.gms.version" android:value="@integer/google_play_services_version" />
        <meta-data android:name="com.google.android.maps.v2.API_KEY" android:value="@string/google_maps_key"/>

        <activity android:name="com.mukulatawnia.emergency2.MapsActivity"
            android:label="@string/title_activity_maps">

        </activity>

        <activity android:name=".Counselling"></activity>
        <activity android:name=".Distress"></activity>
        <activity android:name=".Legal_Aid"></activity>
        <activity android:name=".Shelter"></activity>

        <activity android:name=".Police"></activity>
